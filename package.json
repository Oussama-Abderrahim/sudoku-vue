{
  "name": "sudoku-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:tailwind-watch\"",
    "tailwind-watch": "npx tailwindcss -i ./src/assets/main.css -o ./dist/output.css --watch",
    "build": "npx tailwindcss -i ./src/assets/main.css -o ./dist/output.css && vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.15",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.12",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
